@{
    Layout = null;
}
@model PetaPoco.Page<qch.Models.NewsModel>
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Test1</title>
</head>
<body>
    <div>
        @if (Model != null && Model.Items != null)
        {
            foreach (var item in Model.Items)
            {
                var m = Html.Raw(item.t_News_Contents);
                <span>@m</span>
            }
        }
    </div>
</body>
</html>
